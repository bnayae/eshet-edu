{
  "name": "eshet-edu",
  "version": "1.1.32",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "export": "next build && next export && serve out -l 3001",
    "publish": "next export",
    "lint": "run-s lint:tsx lint:css",
    "lint:tsx": "eslint . --ext .ts,.tsx --fix --quiet",
    "lint:css": "stylelint **/*.tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve": "serve out -l 3001"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "npm run build"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "eslint . --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "add-zero": "^1.0.0",
    "aos": "^2.3.4",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "cross-env": "7.0.2",
    "express": "^4.17.1",
    "framer-motion": "^2.9.1",
    "next": "^9.5.5",
    "next-images": "^1.6.0",
    "react": "^16.13.1",
    "react-device-detect": "^1.14.0",
    "react-dom": "^16.13.1",
    "react-mt-svg-lines": "^0.9.1",
    "react-vivus": "0.0.2",
    "recoil": "0.0.13",
    "styled-components": "^5.2.0",
    "typeface-roboto": "0.0.75",
    "use-sound": "^1.0.2",
    "vivus": "^0.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/aos": "^3.0.3",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.51",
    "@types/react-animate-on-scroll": "^2.1.2",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@types/vivus": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "next-build-id": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
